@using Blog.Model.ViewModels
@model IdentityPermissionViewModel

@Html.HiddenFor(m => m.Id)
@Html.HiddenFor(m => m.Name)
@Html.HiddenFor(m => m.Description)

<div class="checkbox">
    <label>
        @Html.CheckBoxFor(m => m.IsSelected)
        @Html.DisplayFor(m => m.Description)
    </label>
</div>

